{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "webpack": "webpack",
    "server": "webpack-dev-server",
    "start": "set NODE_ENV=production && npm run server",
    "test": "set NODE_ENV=test && karma start --single-run",
    "test:watch": "set NODE_ENV=test && karma start",
    "tsc": "tsc",

    "protractor": "protractor protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "jasmine-core": "2.4.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "^0.1.5",
    "typescript": "^1.9.0-dev.20160603-1.0",
    "typings": "^0.7.9",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "3.0.0-alpha.3",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "angular2-oauth2": "^1.3.8",
    "bootstrap": "^3.3.6",
    "core-js": "^2.2.0",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~1.0.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
